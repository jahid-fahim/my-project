#include<bits/stdc++.h>
using namespace std;

#define ll long long
#define pi 3.14159265
#define pb push_back


int main()
{
    //ll t,x,y,j,i,z,p=0,q=0,d,mx=0;
    //ll a,b,c,d,up,down,l,r,t,t1,t2,ans,ans1,ans2,w,h,w1,w2,h1,h2;
    //char c;
    ll t,sum,j,k,x,sa,sb,p,c,n,i;
    cin>>t;
    //string s;
    while(t--)
    {
        c=0;
        cin>>n;
        char a[n];
        for(i=0; i<n; i++)
        {
            cin>>a[i];
        }
        if(a[n-1]=='0')
        {
            for(i=1; i<=n+1; i++)
            {
                cout<<i<<" ";
            }
        }
        else if(a[0]=='1')
        {
            cout<<n+1<<" ";
            for(i=1; i<=n; i++)
            {
                cout<<i<<" ";
            }
        }
        else
        {
            for(i=0; i<n-1; i++)
            {
                if(a[i]=='0' && a[i+1]=='1')
                {
                    p=i+1;
                    break;
                }
            }
            for(j=1; j<=p; j++)
            {
                cout<<j<<" ";
            }
            cout<<n+1<<" ";
            for(j=p+1; j<=n; j++)
            {
                cout<<j<<" ";
            }
        }
        cout<<endl;
    }

return 0;
}
//Alhamdulillah

