//Type cast
//To type cast a character into integer use int x = (int)character - 48;



#include<bits/stdc++.h>
using namespace std;

#define ll long long
#define pi 3.14159265
#define pb push_back


int main()
{
    ll t,n,p,c,l;
    cin>>t;
    while(t--)
    {
        p=l=1,c=0;
        cin>>n;
        while(n>=p)
        {
            n-=p;
            c++;
            l=(l*2)+1;
            p=(l*(l+1))/2;
        }
        cout<<c<<endl;
    }

 return 0;
}
//Alhamdulillah

