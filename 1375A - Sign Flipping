#include<bits/stdc++.h>
using namespace std;

#define ll long long
#define pi 3.14159265
#define pb push_back


int main()
{
    //ll t,x,y,j,i,z,p=0,q=0,d,mx=0;
    //ll a,b,c,d,up,down,l,r,t,t1,t2,ans,ans1,ans2,w,h,w1,w2,h1,h2;
    //char c;
    ll x,y,m,j,n,i,t;
    cin>>t;
    //string s;
    while(t--)
    {
        ll ans=0;
        cin>>n;
        ll a;
        for(i=0; i<n; i++)
        {
            cin>>a;
            if(i%2==0)
                cout<<abs(a)<<" ";
            else
            {
                if(a>0)
                    cout<<(-a)<<" ";
                else
                    cout<<a<<" ";
            }
        }
        cout<<endl;
    }

 return 0;
}
