#include<bits/stdc++.h>
using namespace std;

#define ll long long
#define pi 3.14159265
#define pb push_back


int main()
{
    int two=0,n,t,c=0,i,j;
    //ll a,b,c,d,up,down,l,r,t,t1,t2,ans,ans1,ans2,w,h,w1,w2,h1,h2;
    //char c;
    //int x,m,j,i,t,c,p;
    cin>>t;
    while(t--)
    {
        c=0;
        string s;
        cin>>n>>s;
        for(i=0; i<n-1; i++)
        {
            if((s[i]=='a' && s[i+1]=='b') || (s[i]=='b' && s[i+1]=='a'))
            {
                c=1;
                break;
            }
        }
        if(c)
            cout<<i+1<<" "<<i+2<<endl;
        else
            cout<<"-1"<<" "<<"-1"<<endl;
    }

 return 0;
}
