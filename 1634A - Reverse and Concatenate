/*int gcd(int a,int b)
{
    if(a%b==0) return b;
    else gcd(b,a%b);
}*/

int main()
{
    int t,n,k;
    cin>>t;
    while(t--)
    {
        cin>>n>>k;
        string s,st;
        cin>>s;
        st = s;
        reverse(st.begin(),st.end());
        if(k==0)cout<<"1"<<endl;
        else if(s==st)cout<<"1"<<endl;
        else cout<<"2"<<endl;
    }

return 0;
}
