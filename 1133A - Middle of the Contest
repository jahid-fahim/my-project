#include<bits/stdc++.h>
using namespace std;

#define ll long long
#define pi 3.14159265
#define pb push_back


int main()
{
    //ll t,x,y,j,i,z,p=0,q=0,d,mx=0;
    //ll n,m,k,i,j,ans=0,t=0,m0;
    int h1,h2,m1,m2,n;
    char c;
    cin>>h1>>c>>m1;
    cin>>h2>>c>>m2;
    n = h1*60+m1+h2*60+m2;
    n/=2;
    if(n/60<10 && n%60<10)
        cout<<"0"<<n/60<<":"<<"0"<<n%60<<endl;
    else if(n/60<10)
        cout<<"0"<<n/60<<":"<<n%60<<endl;
    else if(n%60<10)
        cout<<n/60<<":"<<"0"<<n%60<<endl;
    else
        cout<<n/60<<":"<<n%60<<endl;

return 0;
}
//Alhamdulillah

