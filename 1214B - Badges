#include<bits/stdc++.h>
using namespace std;

#define ll long long
#define pi 3.14159265
#define pb push_back


int main()
{
    int g,b,n,t,c=0;
    //ll a,b,c,d,up,down,l,r,t,t1,t2,ans,ans1,ans2,w,h,w1,w2,h1,h2;
    //char c;
    //int x,m,j,i,t,c,p;
    cin>>b>>g>>n;
    for(int i=0; i<=b; i++)
    {
        for(int j=0; j<=g; j++)
        {
            if(i+j==n)
                c++;
        }
    }
    cout<<c<<endl;
    
return 0;
}
//Alhamdulillah

